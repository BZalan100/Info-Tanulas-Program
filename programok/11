#include <stdio.h>
#include <string.h>
#include <iostream>
using namespace std;
int main()
{
    char szoveg[99];
    fgets(szoveg, sizeof(szoveg), stdin);
    for (int i = 0; i < strlen(szoveg)-1; i++)
    {
        if (szoveg[i] == ' ')
        {
            strcpy(szoveg+i, szoveg+i+1);
            i--;
        }
    }
    cout << szoveg;
    return 0;
}
