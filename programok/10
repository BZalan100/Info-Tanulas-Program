#include <iostream>

using namespace std;

int main()
{
    int n;
    cout << "Egy szamot: "; cin >> n;
    for (int i = 2; i <= n/2; i++)
    {
        if (n % i == 0)
        {
            cout << "Nem egy prim szam";
            return 0;
        }
    }
    cout << "Ez egy prim szam";
    return 0;
}
