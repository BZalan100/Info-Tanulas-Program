#include <stdio.h>
#include <string.h>
#include <iostream>
using namespace std;
int main()
{
    char x[99];
    cout << "szoveg: ";
    fgets(x, sizeof(x), stdin);
    int i, s=0;
    for (i=1; i < strlen(x)-1; i++)
    {
        if (x[i-1] == 'o' && x[i] == 'k')
            s++;
    }
    cout << s << "-szer van benne az hogy 'ok'";
    return 0;
}
