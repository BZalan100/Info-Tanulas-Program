#include <iostream>

using namespace std;

int main()
{
    int arr[99], n;
    cout << "Sorozat hossza: "; cin >> n;
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    int j = 0;
    for (int i = 0; i < n - 1; i++) {
        if (arr[i] != arr[i + 1]) {
            arr[j++] = arr[i];
        }
    }
    arr[j++] = arr[n - 1];
    for (int i = 0; i < j; i++)
    {
        cout << arr[i] << ' ';
    }
    return 0;
}
